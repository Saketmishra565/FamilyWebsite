{
  "builds": [
    {
      "src": "familysite/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "familysite/wsgi.py"
    }
  ],
  "installCommand": "pip install -r requirements.txt",
  "buildCommand": "python familysite/manage.py collectstatic --noinput"
}