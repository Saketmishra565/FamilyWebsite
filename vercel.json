{
  "version": 2,
  "builds": [
    {
      "src": "familysite/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "familysite/wsgi.py"
    }
  ],
  "functions": {
    "familysite/wsgi.py": {
      "runtime": "python3.12", 
      "handler": "familysite.wsgi.application" 
    }
  },
  "installCommand": "pip install -r requirements.txt",
  "buildCommand": "python manage.py collectstatic --noinput"
}