{
  "builds": [
    {
      "src": "familysite/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11",
        "launcherType": "gunicorn",
        "handler": "familysite.wsgi.application"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "familysite/wsgi.py"
    }
  ]
}